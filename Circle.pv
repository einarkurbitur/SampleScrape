size(400, 400)
background(1)
import math
stroke(0)
cx = 200
cy = 200
a = 1.5*math.pi #the degrees of the turn
r = 100 #the length of the radius
x1 = (cx + r * math.cos(a))
y1 = cy + r * math.sin(a)
line (cx, cy,x1,y1)
a = a + 0.5
line (x1,y1, x1 + r * (math.cos(a)), y1 + r * math.sin(a))
a = a - 0.4
line (x1,y1, x1 + r * (math.cos(a)), y1 + r * math.sin(a))

